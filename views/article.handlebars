{{#if articleEntry}}
<h1>
    {{title}}
</h1>
<h3> By {{userName}}</h3>
<h3>{{publishDate}}</h3>



<div id="content">
    {{{content}}}
</div>
<div id="images">

    {{#each images}}
    
    {{/each }}
    <div id="author_info" data-author={{userId}}>
        {{about}}
    </div>
    {{else}}
    <p>No article available.</p>
{{/if}}


 