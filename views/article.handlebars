{{#if articleEntry}}
<h1>
    {{article.Title}}
</h1>
<h3> By {{article.UserID}}</h3>
<h3>{{article.PublishDate}}</h3>



<div id="content">
    {{{article.Content}}}
</div>
<div id="images">

    {{#each images}}

    {{/each }}
    <a href="" id="author_info" data-author={{article.UserID}}>
        author's page
    </a>
    <a href="./editArticle">Edit the article</a>
    <a href="">Delete the article</a>
    {{else}}
    <p>No article available.</p>
    {{/if}}